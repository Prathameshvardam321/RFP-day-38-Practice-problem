let heads = 0
let tails = 0
while(heads<11&&tails<11){
    let flipCoin = Math.floor(Math.random()*2)
    if(flipCoin==0){
        console.log("Heads")
        heads++
    }else{
        console.log("Tails")
        tails++
    }
    if(tails==11){
        console.log("Tails is winner")
    }else if(heads==11){
        console.log("Heads is winner")
    }
}