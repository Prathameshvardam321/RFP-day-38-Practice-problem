function checkPalindrome(number){
    var temp = number
    let reverse = 0
while(number>0.99){
    let remainder = parseInt(number%10)
    reverse = reverse*10+remainder 
    number=number/10
}
if(temp==reverse){
  console.log(temp+" is palindrome number.")  
}else{
    console.log(temp+" is not palindrome number.")  

}

}
