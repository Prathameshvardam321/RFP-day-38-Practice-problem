function checkPrime(inputNumber){
    let c =0;
    for (var i =2;i < inputNumber; i++){
        if(inputNumber%i==0){
            c = 1;
            break
        }
    }
    if(c==0){
        console.log(inputNumber,"is prime number")
        return true
        }else{
            console.log(inputNumber,"is not prime number")

           return false
        }
    }
    checkPrime(97)
